# Подключение к локальной PostgreSQL (docker compose на порту 5433)
DATABASE_URL=postgresql://postgres:postgres@localhost:5433/oneprice?schema=public

# Публичный URL веб-приложения (Mini App)
NEXT_PUBLIC_APP_URL=http://localhost:4000

# URL, который Telegram Bot открывает в кнопке WebApp
WEBAPP_URL=http://localhost:4000

# Токен Telegram бота (получить у @BotFather)
TELEGRAM_BOT_TOKEN=PASTE_TOKEN_HERE

# Telegram user id администратора (кому отправлять уведомления)
ADMIN_TELEGRAM_ID=PASTE_ADMIN_ID_HERE

# Ключ для входа в админку/API
ADMIN_KEY=change_me_admin_key
